<x2 namespace="x2.Links.SocketLink">

    <consts name="SocketLinkEventType">
        <const name="KeepaliveEvent">-10</const>
        <const name="KeepaliveTick">-11</const>

        <const name="HandshakeReq">-20</const>
        <const name="HandshakeResp">-20</const>
        <const name="HandshakeAck">-20</const>

        <const name="SessionKeyReq">-30</const>
        <const name="SessionKeyResp">-31</const>
    </consts>

    <event name="KeepaliveEvent" id="SocketLinkEventType.KeepaliveEvent">
    </event>
    <event name="KeepaliveTick" id="SocketLinkEventType.KeepaliveTick">
        <property name="LinkName" type="string"/>
    </event>

    <event name="HandshakeReq" id="SocketLinkEventType.HandshakeReq">
        <property name="Data" type="bytes"/>
    </event>
    <event name="HandshakeResp" id="SocketLinkEventType.HandshakeResp">
        <property name="Data" type="bytes"/>
    </event>
    <event name="HandshakeAck" id="SocketLinkEventType.HandshakeAck">
    </event>

</x2>